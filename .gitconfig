[user]
    name = iamiokiller
    email = iam.iokiller@gmail.com

[init]
    defaultbranch = main

[alias]
    s = status
    c = commit -am
    co = checkout
    cob = checkout -b
    df = diff --color --color-words

# auto set
[color]
    ui = auto

# my favorite choices
[color "diff"]
    meta = yellow bold
    commit = green bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
    newMoved = cyan dim
    oldMoved = blue dim
    newMovedAlternative = cyan italic
    oldMovedAlternative = blue italic
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
